set.seed(7)
valor <- round(runif(35, min=30, max=100), 2)
# COEFICIENTE DE VARIAÇÃO
sd(valor) / mean(valor)
#j)   Calcule as estatísticas: média, mediana, variância, desvio padrão e
#     coeficiente de variação;
# DADOS ORDENADOS:
sort(valor)
# MÉDIA:
mean(valor)
# MEDIANA:
median(valor)
# VARIÂNCIA:
var(valor)
# DESVIO PADRÃO:
sd(valor)
# COEFICIENTE DE VARIAÇÃO
sd(valor) / mean(valor)
